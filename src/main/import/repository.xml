                                                        <j:translation_en jcr:language="en"
                                                         jcr:primaryType="jnt:translation"
                                                         text="&lt;div class=&#34;span4&#34;&gt;&#xA;&lt;ul class=&#34;footer_links&#34;&gt;&#xA; &lt;li&gt;&lt;a href=&#34;index.html#&#34;&gt;Contacts&lt;/a&gt;&lt;/li&gt;&#xA; &lt;li&gt;&lt;a href=&#34;index.html#&#34;&gt;Help Desk&lt;/a&gt;&lt;/li&gt;&#xA; &lt;li&gt;&lt;a href=&#34;index.html#&#34;&gt;Privacy Policy&lt;/a&gt;&lt;/li&gt;&#xA;&lt;/ul&gt;&#xA;&lt;/div&gt;&#xA;&#xA;&lt;div class=&#34;span8&#34;&gt;&#xA;&lt;div class=&#34;right affiliate_links&#34;&gt;&lt;a alt=&#34;National Aeronautics and Space Administration&#34; href=&#34;http://www.nasa.gov&#34; title=&#34;National Aeronautics and Space Administration&#34;&gt;&lt;img alt=&#34;stsci_footer.png&#34; src=&#34;##doc-context##/{workspace}/##ref:link1##&#34; style=&#34;width: 60px; height: 60px;&#34; /&gt;&lt;img alt=&#34;nasa_footer.png&#34; src=&#34;##doc-context##/{workspace}/##ref:link2##&#34; /&gt;&lt;/a&gt; &lt;a alt=&#34;The European Space Agency&#34; href=&#34;http://www.esa.int/&#34; title=&#34;The European Space Agency&#34;&gt;&lt;img alt=&#34;esa_footer.png&#34; src=&#34;##doc-context##/{workspace}/##ref:link3##&#34; /&gt;&lt;/a&gt;&lt;/div&gt;&#xA;&#xA;&lt;div class=&#34;right site_disclaimer&#34;&gt;&#xA;&lt;p&gt;James Webb Space Telescope is operated by Space Telescope Institute&lt;br /&gt;&#xA;on the campus of John Hopkins University in Baltimore.&lt;/p&gt;&#xA;&lt;/div&gt;&#xA;&lt;/div&gt;&#xA;"/>
                                                        <j:referenceInField_text_en_3 j:fieldName="text_en"
                                                                     j:reference="/modules/pilot-template/templates/files/img/esa_footer.png"
                                                                     jcr:primaryType="jnt:referenceInField"/>
                                                        <j:referenceInField_text_en_2 j:fieldName="text_en"
                                                                     j:reference="/modules/pilot-template/templates/files/img/nasa_footer.png"
                                                                     jcr:primaryType="jnt:referenceInField"/>
                                                        <j:referenceInField_text_en_1 j:fieldName="text_en"
                                                                     j:reference="/modules/pilot-template/templates/files/img/stsci_footer.png"
                                                                     jcr:primaryType="jnt:referenceInField"/>
                                                    </rich-text>
